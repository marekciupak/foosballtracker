<table class="table">
  <thead>
    <tr>
      <th>Winner</th>
      <th>Loser</th>
      <th>Date</th>
      <th>Score</th>
    </tr>
  </thead>

  <tbody>
    <% @matches.each do |match| %>
      <tr>
        <td><%= match.winner.fullname %></td>
        <td><%= match.loser.fullname %></td>
        <td><%= match.date %></td>
        <td>10:<%= match.loser_score %></td>
      </tr>
    <% end %>
  </tbody>
</table>